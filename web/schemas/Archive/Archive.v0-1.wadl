	    <resource id="timeline" path="timeline">
	        <doc title="Timeline events collection">
			</doc>
			<method name="GET" id="FindEventsUseCase">
			    <doc title="Timeline events collection request">
				</doc>
				<request>
					<param name="Accept" style="header">
						<doc>application/xml</doc>
					</param>
					<param name="Accept" style="header">
						<doc>application/json</doc>
					</param>
			    </request>
			    <response status="200">
					<param name="Etag" style="header" />
					<representation mediaType="application/xml; charset=UTF-8" element="evs:Events">
						<doc title="OK" >Accept header application/xml</doc>
					</representation>
					<representation mediaType="application/json; charset=UTF-8">
						<doc title="OK">Accept header application/json</doc>
					</representation>
				</response>
				<response status="500">
					<representation mediaType="text/plain">
						<doc title="Internal server error">
						    On internal server errors
						</doc>
					</representation>
				</response>
			</method>
	    </resource>
		<resource id="staff" path="staff">
